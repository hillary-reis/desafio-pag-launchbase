{% extends 'layout.njk' %}

{% block head %}
  <title> {{ about.title }} </title>
{% endblock %}

{% block content %}
  <div id="content">
    <img src=" {{ about.img }} "  alt=" {{ about.alt }} ">
    <h1> {{ about.title }} </h1>
    <h2> {{ about.description }} </h2>
    <div class="list"> {{ about.list }} </div>
  </div>

  <footer>
    <div class="links" id="footer">
      {% for link in about.links %}
        <a href="{{ link.url }}" target="_blank"> {{ link.name }} </a>
      {% endfor %}
    </div>
  </footer>
{% endblock %}